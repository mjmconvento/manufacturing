{% import "CatalystTemplateBundle::form.html.twig" as f %}
<!-- add entry modal -->
<div id="add-entry-modal" class="modal fade" role="dialog">
	<form id="add-entry-form" method="post" action="{{ path('bgn_entry_ajax_add')}}">
		<div class="modal-dialog">
			<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title">Supply</h4>
			</div>
			<div class="modal-body form-horizontal">
                <div class="form-body">
                    {% set prefix = 'aa' %}
                    {{ f.group_select('Description', 'name', '', '', 3, 8, false, prefix) }}                                 
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button id="add-entry-submit" type="button" class="btn btn-primary">Add Supply</button>
            </div>
            </div>
		</div>
	</form>
</div>
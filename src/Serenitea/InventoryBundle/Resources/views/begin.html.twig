<button type="button" id="add-entry" class="btn btn-primary" data-toggle="modal" data-target="#add-entry-modal">Add Supply</button>
<div class="row table-responsive" style="padding-top:10px">
	<div class="col-md-12">
		<table class="table table-striped table-bordered table-hover" aria-describedby="list_table_info">
			<thead>
                <tr>
	                <th>Item Code</th>
	                <th>Description</th>
	                <th>Quantity</th>
	                <th>Specs</th>
	                <th>Expiration</th>
	                <th style="width: 60px"></th>
                </tr>
            </thead>
            <tbody id="from_table">            	
            </tbody>
		</table>
	</div>
</div>
</div>
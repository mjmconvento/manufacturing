{% extends "CatalystTemplateBundle:Object:list.dynamic.html.twig" %}

{% block content %}
<div class="row">
    <div class="col-md-12">
        <div class="portlet box blue-hoki">
            <div class="portlet-title">
                <div class="caption">{{ list_title }}</div>
                <div class="actions">
                </div>
            </div>
            <div class="portlet-body">
                <div class="table-toolbar">
                </div>
                <table class="table table-striped table-bordered table-hover" id="list_table">
                    <thead>
                        <tr>
                        {% for col in grid_cols %}
                            <th>{{ col.getLabel }}</th>
                        {% endfor %}
                        </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <td>PO-2014-0001</td>                            
                        <td>11/26/2014</td>                                                       
                        <td>12/01/2014</td>
                        <td>Makati-Rada</td>                            
                        <td></td>
                        <td><a href="{{ path('ser_rlist_edit')}}{##btn_en_add#}" class="btn default btn-xs blue-madison">View</a></td>  
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock %}